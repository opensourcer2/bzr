<indexterm><primary>glade_design_layout_widget_event</primary></indexterm><programlisting><link linkend="gboolean">gboolean</link>            glade_design_layout_widget_event    (<link linkend="GladeDesignLayout">GladeDesignLayout</link> *layout,
